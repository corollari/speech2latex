{
	"name": "Speech2Latex",
	"version": "0.1",
	"description": "Speech in, latex out",
	"manifest_version": 2,
	"background":{
		"scripts":["bg.js"]
        },
	"browser_action": {
		"default_icon": {
			"16": "icons/16.png",
			"24": "icons/24.png",
			"32": "icons/32.png"
		},
		"default_title": "Start recording"
	},
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": ["activeTab"]
}
